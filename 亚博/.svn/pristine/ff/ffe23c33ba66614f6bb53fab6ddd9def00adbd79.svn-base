.UI-select{
    font-size: 12px;
}
.UI-select,.UI-select .UI-select-listico,.UI-select .UI-options li.active span{-moz-box-sizing:border-box;-webkit-box-sizing:border-box;-o-box-sizing:border-box;-ms-box-sizing:border-box;box-sizing: border-box;}
.UI-select{box-shadow: inset 0 1px 1px rgba(0,0,0,.1);text-align: left;position: relative;height:26px;border: 1px #ddd solid;height: 40px;border-radius: 3px;display:inline-block;background:#fff;line-height:24px;}
.UI-select .UI-select-listico{text-align:center;cursor: pointer;display: inline-block;height: 100%;position: absolute;right: 0;top: 0;width: 26px;}
.UI-select .UI-select-listico em{position:absolute;left:8px;bottom:12px;display: inline-block;border:4px solid transparent;border-top:4px solid #ddd;border-bottom:none;}
.UI-select .UI-select-listico b{position:absolute;left:8px;top:12px;display: inline-block;border:4px solid transparent;border-bottom:4px solid #ddd;border-top:none;}
.UI-select .UI-options{z-index:101;color:#555;max-height:280px;overflow-y:auto;display:none;width: 100%;border: 1px solid #d1d1cd;background-color: #fff;position: absolute;top:44px;left:-1px;border-radius:3px;  box-shadow: 0 5px 10px rgba(0,0,0,.1);}
.UI-select .UI-options.showbox{display:block;}
.UI-select .UI-options li{cursor: pointer;height: 32px;line-height: 32px;padding-left: 12px;}
.UI-select .UI-options li:hover{color:#666;background-color: #eeeeee;}
.UI-select .UI-options li.active{
    font-weight:bold;padding-left: 16px;background: #f8f8f8;
}
.UI-select .UI-select-cur{color:#666;width:100%;-moz-box-sizing:border-box;-webkit-box-sizing:border-box;-o-box-sizing:border-box;-ms-box-sizing:border-box;box-sizing: border-box;cursor:pointer;font-size: 14px;display: inline-block;padding-left:12px;line-height: 38px;}