        <span class="inlilne-block border_red1 red p5_25" ng-click="data.zybzTab='lzy' " ng-class="{tm_active:data.zybzTab=='lzy'}">六中一</span>
    </div>
    <div ng-show="data.zybzTab=='wbz'">
        <div>
            <div class="hgt40 line_hgt40" style="background: #BD2929">
                <div data-ng-repeat="(key, value) in range(0,cols)" class="inline-block w20per">
                <div class="inline-block w30per">号码</div>
                <div class="inline-block w30per">赔率</div>
                <div class="inline-block w30per">投注金额</div>
                </div>
            </div>
        </div>
        <div>
            <div class="hgt36 line_hgt36" style="background: #EAEAEA" data-ng-repeat="(index, item) in range(0,wbz_pls.length/cols)">
              <div data-ng-repeat="(key, value) in range(0,cols)" class="inline-block w20per" ng-show="wbz_pls[index*cols+key]">
                <div class="inline-block w30per"><span class="inline-block" ng-class="{redCircle:ifBgCircle(wbz_pls[index*cols+key].number),redBlock:!ifBgCircle(wbz_pls[index*cols+key].number),greenBlock:wbz_pls[index*cols+key].number=='绿波',blueBlock:wbz_pls[index*cols+key].number=='蓝波'}">{{wbz_pls[index*cols+key].number}}</span></div>
                <div class="black inline-block w30per">{{wbz_pls[index*cols+key].pl}}</div>
                <div class="inline-block w30per">
                    <input type="checkbox" style="vertical-align: middle;" value="1" class="w40 green" ng-class="{'has-error': data.inputErrors[wbz_pls[index*cols+key].id] }" ng-model="data.betMoney[wbz_pls[index*cols+key].id]" ng-change="fun.zybzCodeSelected(wbz_pls[index*cols+key])"/>
                </div>
              </div>
            </div>
