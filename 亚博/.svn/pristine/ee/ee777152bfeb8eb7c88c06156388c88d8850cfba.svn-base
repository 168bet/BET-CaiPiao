<div class="w1000 m0auto text_center fz14 white" style="padding:20px 0 0 0;">
 
    <table class="black">
        <thead>
            <tr class="hgt40 line_hgt40 border_b1 border_l1" style="background: #CCCCCC">
                <td class="border_r1">百XX</td>
                <td class="border_r1">勾选</td>
                <td class="border_r1">X拾X</td>
                <td class="border_r1">勾选</td>
                <td class="border_r1">XX个</td>
                <td class="border_r1">勾选</td>
            </tr>
        </thead>
        <tbody>
            <tr class="hgt36 line_hgt36 border_b1 border_l1" style="background: #F1F1F1" ng-repeat="(index,item) in range(0,10)">
                <td class="border_r1">{{index}}</td>
                <td class="border_r1">
                    <input type="checkbox" ng-model="data.betMoney['bwNum'][index]" class="middle">
                </td>
                <td class="border_r1">{{index}}</td>
                <td class="border_r1">
                    <input type="checkbox" class="middle" ng-model="data.betMoney['swNum'][index]">
                </td>
                <td class="border_r1">{{index}}</td>
                <td class="border_r1">
                    <input type="checkbox" class="middle" ng-model="data.betMoney['gwNum'][index]">
                </td>
            </tr>
        </tbody>
    </table>

</div>
