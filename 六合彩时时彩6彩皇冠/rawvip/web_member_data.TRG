TYPE=TRIGGERS
triggers='CREATE DEFINER=`root`@`localhost` TRIGGER `AFTER_DELETE_web_member_data` AFTER DELETE ON `web_member_data` \n    FOR EACH ROW BEGIN\n	delete from ka_mem where kauser=OLD.UserName;\n    END' 'CREATE DEFINER=`root`@`localhost` TRIGGER `AFTER_UPDATE_web_member_data` AFTER UPDATE ON `web_member_data` \n    FOR EACH ROW BEGIN\n	SET @Idanid=(select ID from web_agents_data where UserName=NEW.Agents);\n	SET @Izongid=(select ID from web_agents_data where UserName=NEW.World);\n	SET @Iguanid=(select ID from web_agents_data where UserName=NEW.Corprator);\n	UPDATE ka_mem set kapassword=NEW.PassWord,xm=NEW.Alias,look=0,guanid=@Iguanid,zongid=@Izongid,danid=@Idanid,guan=NEW.Corprator,zong=NEW.World,dan=NEW.Agents,cs=NEW.Credit,ts=NEW.Money WHERE kauser=NEW.UserName;\n    END' 'CREATE DEFINER=`root`@`localhost` TRIGGER `AFTER_INSERT_web_member_data` AFTER INSERT ON `web_member_data` \n    FOR EACH ROW BEGIN\n	SET @Idanid=(select ID from web_agents_data where UserName=NEW.Agents);\n	SET @Izongid=(select ID from web_agents_data where UserName=NEW.World);\n	SET @Iguanid=(select ID from web_agents_data where UserName=NEW.Corprator);\n	insert into ka_mem set kauser=NEW.UserName,kapassword=NEW.PassWord,xm=NEW.Alias,look=0,guanid=@Iguanid,zongid=@Izongid,danid=@Idanid,guan=NEW.Corprator,zong=NEW.World,dan=NEW.Agents,adddate=New.AddDate,cs=NEW.Credit,ts=NEW.Money;\n    END'
sql_modes=0 0 0
definers='root@localhost' 'root@localhost' 'root@localhost'
